{
  "name": "server",
  "type": "module",
  "packageManager": "yarn@4.4.1",
  "engines": {
    "node": "^22"
  },
  "scripts": {
    "i": "yarn install",
    "b": "tsc",
    "t": "jest",
    "l": "yarn g:lint .",
    "dev": "nodemon server.ts",
    "serve": "tsx server.ts",
    "build": "yarn g:build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "mongodb": "^6.8.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^22.5.0",
    "jest": "*",
    "nodemon": "^3.1.4",
    "tsx": "*",
    "typescript": "*"
  },
  "nodemonConfig": {
    "verbose": false,
    "execMap": {
      "ts": "tsx"
    }
  }
}
